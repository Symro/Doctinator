<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.doctinator.doctinator" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CallListActivity"
            android:label="@string/title_activity_call_list"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.doctinator.doctinator.MainActivity" />
        </activity>
        <activity
            android:name=".ActionList"
            android:label="@string/title_activity_action_list"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.doctinator.doctinator.MainActivity" />
        </activity>
        <activity
            android:name=".FindPlace"
            android:label="@string/title_activity_find_place"
            android:parentActivityName=".ActionList" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.doctinator.doctinator.ActionList" />
        </activity>
        <activity
            android:name=".FirstHelpActivity"
            android:label="@string/title_activity_first_help"
            android:parentActivityName=".ActionList" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.doctinator.doctinator.ActionList" />
        </activity>
    </application>

</manifest>
